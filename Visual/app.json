{
  "expo": {
    "name": "swipe-madrid",
    "slug": "swipe-madrid",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/madrizzz.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/madrizzz.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/madrizzz.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.swipemadrid"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": false
          }
        }
      ]
    ]
  }
}
